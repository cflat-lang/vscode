{
	"name": "cflat",
	"displayName": "cflat",
	"description": "CFLat Language Integration",
	"version": "0.0.1",
	"publisher": "matheuslessa",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Programming Languages"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/cflat-lang/vscode.git"
	},
	"contributes": {
		"languages": [
			{
				"id": "cflat",
				"aliases": [
					"Cb",
					"CFlat"
				],
				"extensions": [
					".cb"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "cflat",
				"scopeName": "source.cb",
				"path": "./syntaxes/cflat.tmLanguage.json"
			}
		]
	}
}